import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
			Locale.setDefault(Locale.US);
			Scanner sc = new Scanner(System.in);
			
			
			 
			int ini = sc.nextInt();
			int fim = sc.nextInt();
			int duracao = 24;
			
			if (ini >= fim){
				duracao += (fim - ini);
			}	
			else {
				duracao = (fim - ini);	
			}
				System.out.printf("O JOGO DUROU "+duracao+" HORA(S)");
			
			sc.close();
			}	
}
